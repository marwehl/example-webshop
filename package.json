{
  "name": "parcel-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for neuefische students",
  "main": "./src/index.html",
  "scripts": {
    "start": "run-p serve watch",
    "serve": "parcel ./src/index.html --open",
    "watch": "parcel watch ./src/*.html",
    "build": "parcel build ./src/*.html"
  },
  "dependencies": {
    "@babel/core": "7.2.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.6.1",
    "posthtml-img-autosize": "^0.1.4",
    "posthtml-modules": "^0.4.2",
    "sass": "^1.21.0"
  }
}
